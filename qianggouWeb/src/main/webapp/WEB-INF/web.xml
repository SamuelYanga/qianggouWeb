<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">

	<display-name>Archetype Created Web Application</display-name>

	<servlet>
	    <servlet-name>ReloadUserInfoServlet</servlet-name>
	    <servlet-class>com.web.nike.servlet.ReloadUserInfoServlet</servlet-class>
	    <load-on-startup>1</load-on-startup>
	</servlet>

	<servlet>
	    <servlet-name>QianggouServlet</servlet-name>
	    <servlet-class>com.web.nike.servlet.QianggouServlet</servlet-class>
	    <load-on-startup>1</load-on-startup>
	</servlet>
	
	<servlet-mapping>
	    <servlet-name>ReloadUserInfoServlet</servlet-name>
	    <url-pattern>/servlet/ReloadUserInfoServlet</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
	    <servlet-name>QianggouServlet</servlet-name>
	    <url-pattern>/servlet/QianggouServlet</url-pattern>
	</servlet-mapping>
	
	
	
	
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>